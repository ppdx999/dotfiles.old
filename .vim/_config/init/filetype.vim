if !has("autocmd")
	finish
endif

"#########  Define filetype  ##########
autocmd BufNewFile,BufRead *.md	set filetype=markdown
autocmd BufNewFile,BufRead *.txt set filetype=text
autocmd BufRead,BufNewFile * if !did_filetype() && getline(1) =~# '@startuml\>'| setfiletype plantuml | endif
autocmd BufRead,BufNewFile *.pu,*.uml,*.plantuml,*.puml,*.iuml set filetype=plantuml

"#########  Read specific file  ##########

" My-defined filetype
autocmd FileType txt           if filereadable(expand('~/.vim/_config/filetype/txt.vim'))               | source ~/.vim/_config/filetype/txt.vim               | endif 
autocmd FileType plantuml      if filereadable(expand('~/.vim/_config/filetype/plantuml.vim'))          | source ~/.vim/_config/filetype/plantuml.vim          | endif 

